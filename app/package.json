{
  "name": "agenda3",
  "version": "3.4.0",
  "private": true,
  "scripts": {
    "start": "meteor --exclude-archs 'web.browser.legacy, web.cordova' --settings ../config/settings.development.json",
    "start-dev": "MONGO_URL=mongodb://localhost:3001/meteor meteor run --exclude-archs 'web.browser.legacy, web.cordova' --settings ../config/settings.development.json --port 3030",
    "test": "meteor test --once --exclude-archs 'web.browser.legacy, web.cordova' --driver-package meteortesting:mocha --allow-superuser",
    "test-app": "TEST_WATCH=1 meteor test --full-app --driver-package meteortesting:mocha --settings ../config/settings.development.json",
    "lint": "eslint . --ext js,jsx",
    "lintfix": "eslint --fix . --ext js,jsx",
    "visualize": "meteor --production --extra-packages bundle-visualizer --settings ../config/settings.development.json"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.8",
    "@fullcalendar/core": "^5.10.1",
    "@fullcalendar/daygrid": "^5.10.1",
    "@fullcalendar/interaction": "^5.10.1",
    "@fullcalendar/list": "^5.10.1",
    "@fullcalendar/moment-timezone": "^5.10.1",
    "@fullcalendar/react": "^5.10.1",
    "@fullcalendar/timegrid": "^5.10.1",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "axios": "^0.21.4",
    "babel-eslint": "^10.1.0",
    "bcrypt": "^5.0.1",
    "buffer": "^6.0.3",
    "chai": "^4.3.6",
    "faker": "^5.5.3",
    "ical-generator": "^2.2.0",
    "material-icons": "^1.11.1",
    "meteor-node-stubs": "^1.2.1",
    "moment": "^2.29.1",
    "node-ical": "^0.13.0",
    "prop-types": "^15.8.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.3.0",
    "react-toastify": "^7.0.4",
    "simpl-schema": "^1.12.0",
    "sinon": "^11.1.2"
  },
  "meteor": {
    "mainModule": {
      "client": "client/main.js",
      "server": "server/main.js"
    },
    "testModule": "tests/main.js"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-import-resolver-meteor": "^0.4.0",
    "eslint-plugin-i18n": "^2.1.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx": "^0.1.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-meteor": "^7.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-standard": "^4.1.0",
    "prettier": "^2.6.0"
  }
}
